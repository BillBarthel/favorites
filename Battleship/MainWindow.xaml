<Window x:Class="Battleship.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Battleship"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="SignIn" Content="SignIn" HorizontalAlignment="Left" Margin="159,167,0,0" VerticalAlignment="Top" Width="75" Click="SignIn_Click"/>
        <Button x:Name="Register" Content="Register" HorizontalAlignment="Left" Margin="250,167,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.013,-0.3" Click="Register_Click"/>
        <TextBox x:Name="Username" HorizontalAlignment="Left" Height="23" Margin="159,120,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="166" TextChanged="Username_TextChanged"/>
        <Label Content="Login" HorizontalAlignment="Left" Margin="226,24,0,0" VerticalAlignment="Top" Width="58" FontSize="16"/>

    </Grid>
</Window>
